type SendVerificationCodeRegisterRequest {
	Destination string `json:"destination"`
}

type SendVerificationCodeRegisterResponse {}

type SendVerificationCodePWDRequest {
	Destination string `json:"destination"`
}

type SendVerificationCodePWDResponse {}

@server (
	timeout: 5s
)
service Email {
	@handler SendVerificationCodeRegister
	post /api/email/sendverificationcoderegister (SendVerificationCodeRegisterRequest) returns (SendVerificationCodeRegisterResponse)

	@handler SendVerificationCodePWD
	post /api/email/sendverificationcodepwd (SendVerificationCodePWDRequest) returns (SendVerificationCodePWDResponse)
}

